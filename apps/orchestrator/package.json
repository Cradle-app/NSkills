{
  "name": "@dapp-forge/orchestrator",
  "version": "0.1.0",
  "type": "module",
  "description": "DappForge orchestrator API for blueprint validation and code generation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "dependencies": {
    "@cradle/plugin-config": "workspace:*",
    "@dapp-forge/blueprint-schema": "workspace:*",
    "@dapp-forge/plugin-sdk": "workspace:*",
    "@dapp-forge/plugins": "workspace:*",
    "@dapp-forge/github": "workspace:*",
    "@fastify/cors": "^9.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "fastify": "^4.26.0",
    "memfs": "^4.6.0",
    "nanoid": "^5.0.0",
    "pino": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}

