{
  "name": "dapp-forge",
  "version": "0.1.0",
  "private": true,
  "description": "Web3 Dapp Blueprint workflow engine and repo generator",
  "scripts": {
    "dev": "turbo run dev --concurrency=20",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "start:orchestrator": "cd apps/orchestrator && NODE_PATH=../../node_modules:./node_modules:$NODE_PATH node dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}