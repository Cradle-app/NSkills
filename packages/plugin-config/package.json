{
    "name": "@cradle/plugin-config",
    "version": "0.1.0",
    "description": "Centralized plugin configuration and compatibility registry for Cradle",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "lint": "eslint src/",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {},
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {},
    "files": [
        "dist"
    ],
    "license": "MIT"
}